//java
class Solution {
    public int search(int[] nums, int target) {
        int ans=1;
        int index=0;
        for(int i=0;i<nums.length;i++){
            if(nums[i]==target){
                ans=0;
                index=i;
                break;
            }
        }
        if(ans==0)
            return index;
        return -1;  
    }
}
